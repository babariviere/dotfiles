export GTK_IM_MODULE=ibus
export XMODIFIERS=@im=ibus
export QT_IM_MODULE=ibus

export BROWSER="firefox-nightly"
export EDITOR="nvim"
export READER="zathura"
export VISUAL="nvim"

export GOPATH="$HOME/projects/go"
export FLUTTER_SDK="$HOME/flutter"
export DART_SDK="$FLUTTER_SDK/bin/cache/dart-sdk"
export ANDROID_ROOT="$HOME/android"
export ANDROID_HOME="$ANDROID_ROOT/sdk"
export VULKAN_SDK="$HOME/sdk/vulkan/1.1.82.1/x86_64"

export PATH="$ANDROID_ROOT/studio/bin:$ANDROID_HOME/tools:$ANDROID_HOME/tools/bin:$HOME/.bin:$HOME/.local/bin:$HOME/.yarn/bin:$HOME/.pub-cache/bin:$FLUTTER_SDK/bin:$DART_SDK/bin:$GOPATH/bin:$HOME/.cargo/bin:$PATH"

export NDK_HOME="/opt/android-ndk"
export WINEDLLOVERRIDES="winemenubuilder.exe=d"

export TEXINPUTS="~/libs/tex:$(kpsepath tex)"
export KUBECONFIG="$HOME/.kube/config"

export XDG_MUSIC_DIR=/data/Music
export DESKTOP_SESSION=gnome
export QT_QPA_PLATFORMTHEME=gtk2

export _JAVA_AWT_WM_NONREPARENTING=1
